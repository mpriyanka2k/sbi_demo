{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "SBI Demo Dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart",
      "flutterMode": "debug",
      "toolArgs": ["--flavor", "dev"]
    },
    {
      "name": "SBI Demo Staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_staging.dart",
      "flutterMode": "debug",
      "toolArgs": ["--flavor", "staging"]
    },
    {
      "name": "SBI Demo",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_prod.dart",
      "flutterMode": "release",
      "toolArgs": ["--flavor", "prod"]
    }
  ]
}
